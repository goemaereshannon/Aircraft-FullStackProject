      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: identityservicesaircraft
    ports:
    - published: 32808
      target: 80
    - target: 80
    - target: 443
    volumes:
    - C:\Users\alexi\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\alexi\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
