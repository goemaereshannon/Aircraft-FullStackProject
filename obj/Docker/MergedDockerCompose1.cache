services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Password123
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1833
      target: 1433
    volumes:
    - C:\Users\alexi\AppData\Roaming\DockerVolumes\sqlserver:/var/opt/mssql/data:rw
  identityservices:
    build:
      context: C:\Users\alexi\Documents\School\MCT\FlightReservation\Aircraft-FullStackProject
      dockerfile: IdentityServices/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: IdentityServices
      target: base
    container_name: IdentityServicesAircraft
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: identityservicesaircraft:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/IdentityServices.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 32808
      target: 80
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\alexi\Documents\School\MCT\FlightReservation\Aircraft-FullStackProject\IdentityServices:/app:rw
    - C:\Users\alexi\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\alexi\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\alexi\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\alexi\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\alexi\Documents\School\MCT\FlightReservation\Aircraft-FullStackProject:/src:rw
version: '3.4'